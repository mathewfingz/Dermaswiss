{{ 'custom-content.css' | asset_url | stylesheet_tag }}
{%- liquid
  assign section_id = section.settings.custom_id | default: section.id
  assign custom_class = section.settings.custom_class
  assign bg_image = section.settings.bg_image

  unless custom_class == blank
    assign custom_class = custom_class | prepend: ' '
  endunless

  assign container = section.settings.container

  assign swipe_on_mobile = section.settings.enable_swipe_mobile
  assign swipe_on_tablet = false
  assign swipe_classes = ''

  if swipe_on_mobile
    if section.blocks.size == 3
      assign swipe_on_tablet = true
    elsif section.blocks.size > 2
      assign column_size_modulo = section.blocks.size | modulo: 2
      if column_size_modulo > 0
        assign swipe_on_tablet = true
      endif
    endif
  endif

  if swipe_on_mobile
    assign swipe_classes = ' f-swiper f-swiper--' | append: section.settings.column_mobile | append: '-cols'
  endif

  if swipe_on_tablet
    assign swipe_classes = swipe_classes | append: ' lg-down-swiper'
  endif

  if swipe_classes != blank
    assign swipe_classes = swipe_classes | append: ' f-swiper--no-scrollbar'
  endif

  capture styles
    if section.blocks.size > 9
      echo '--pagination-width: 4rem;'
    else
      echo '--pagination-width: 2.5rem;'
    endif
  endcapture

  assign loading = 'lazy'
  assign fetchpriority = 'low'
  if section.index < 2
    assign loading = 'eager'
    assign fetchpriority = 'high'
  endif

  assign section_motion_delay = 0
  if section.settings.heading != blank
    assign section_motion_delay = section_motion_delay | plus: 50
  endif
  if section.settings.subheading != blank
    assign section_motion_delay = section_motion_delay | plus: 50
  endif
  if section.settings.description != blank
    assign section_motion_delay = section_motion_delay | plus: 50
  endif
-%}
{% render 'divider',
  show_divider: section.settings.show_section_divider,
  divider_width: section.settings.divider_width
%}
<div
  id="{{ section_id }}"
  class="f-custom relative f-section-padding color-{{ section.settings.color_schema }}{{ custom_class }}"
  data-section-type="f-custom-content"
  data-section-id="{{ section.id }}"
  style="--section-padding-top: {{ section.settings.padding_top }}px; --section-padding-bottom: {{ section.settings.padding_bottom }}px;"
>
  {%- if bg_image != blank -%}
    <div
      class="bg-image"
      style="--aspect-ratio: {{ bg_image.aspect_ratio }}"
    >
      {%- liquid
        assign image_height = bg_image.width | divided_by: bg_image.aspect_ratio
      -%}
      {{
        bg_image
        | image_url: width: 1500
        | image_tag:
          loading: loading,
          fetchpriority: fetchpriority,
          width: bg_image.width,
          height: image_height,
          sizes: '100vw',
          widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840',
          alt: bg_image.alt
        | escape
      }}
    </div>
  {%- endif -%}
  <div class="{{ container }}">
    {% render 'index-section-header', section_settings: section.settings %}
    <div class="f-flex flex-wrap f-grid--gap-{{ section.settings.column_gap }} f-grid--row-gap-{{ section.settings.row_gap }}{{ swipe_classes }}">
      {%- for block in section.blocks -%}
        {%- liquid
          assign vertical_alignment = block.settings.alignment | default: 'center'

          assign block_class = ''
          case block.settings.width
            when '16%'
              assign block_class = block_class | append: ' w-full md:w-1/2 lg:w-2/12'
            when '25%'
              assign block_class = block_class | append: ' w-full md:w-1/2 lg:w-1/4'
            when '33%'
              assign block_class = block_class | append: ' w-full md:w-1/2 lg:w-1/3'
            when '50%'
              assign block_class = block_class | append: ' w-full md:w-1/2 lg:w-1/2'
            when '66%'
              assign block_class = block_class | append: ' w-full md:w-1/2 lg:w-8/12'
            when '75%'
              assign block_class = block_class | append: ' w-full md:w-1/2 lg:w-9/12'
            when '83%'
              assign block_class = block_class | append: ' w-full md:w-1/2 lg:w-10/12'
            when '100%'
              assign block_class = block_class | append: ' w-full'
          endcase

          if block.settings.block_custom_class != blank
            assign block_class = block_class | append: ' ' | append: block.settings.block_custom_class
          endif

          assign animation_effect = settings.animations
          assign animated = false
          if animation_effect != 'none'
            assign animated = true
            if block.settings.animations != 'inherit'
              assign animation_effect = block.settings.animations
            endif
          endif
          assign motion_delay = section_motion_delay
        -%}
        <div
          id="f-custom-block--{{ block.id }}"
          class="f-column {{ block_class }}"
          data-block-type="{{ block.type }}"
          {{ block.shopify_attributes }}
        >
          <div class="f-custom__block f-custom__block--{{ block.type }} h-full md:flex items-{{ vertical_alignment  }} text-{{ block.settings.align_text }}{% if block.settings.color_scheme %} color-{{ block.settings.color_scheme }}{% endif %}{% if block.settings.mobile_color_scheme %} md-down:color-{{ block.settings.mobile_color_scheme }}{% endif %}">
            {%- case block.type -%}
              {%- when 'image' -%}
                <motion-element
                  data-motion="{{ animation_effect }}"
                  data-motion-delay="{{ motion_delay }}"
                  class="block w-full overflow-hidden"
                >
                  {%- if block.settings.image != blank -%}
                    {%- liquid
                      assign tag = 'div'
                      if block.settings.link != blank
                        assign tag = 'a'
                      endif
                    -%}
                    <{{ tag }}
                      class="block f-card w-full{% if block.settings.link != blank %} f-image-hover-effect__{{settings.image_hover_effect}}{% endif %}"
                      {% if block.settings.link != blank %}
                        href="{{ block.settings.link }}"
                      {% endif %}
                    >
                      {% render 'responsive-image',
                        image: block.settings.image,
                        section_index: section.index,
                        index: forloop.index
                      %}
                    </{{ tag }}>
                  {%- else -%}
                    <div class="f-image">
                      {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                    </div>
                  {%- endif -%}
                </motion-element>
              {%- when 'text' -%}
                <div
                  class="f-text-blocks w-full f-blocks-padding f-align-content-container--{{ block.settings.align_content_container }}"
                  style="--padding-left: {{ block.settings.padding_left }}px;--padding-right: {{ block.settings.padding_right }}px;--mobile-padding-left: {{ block.settings.mobile_padding_left }}px;--mobile-padding-right: {{ block.settings.mobile_padding_right }}px;"
                >
                  {%- if block.settings.subheading != blank -%}
                    <motion-element
                      data-motion="{{ animation_effect }}"
                      data-motion-delay="{{ motion_delay }}"
                      class="block mega-subtitle"
                    >
                      {{ block.settings.subheading }}
                    </motion-element>
                    {%- assign motion_delay = motion_delay | plus: 50 -%}
                  {%- endif -%}
                  {%- if block.settings.title != blank -%}
                    {%- liquid
                      assign inline_css = ''

                      if block.settings.heading_color != blank and block.settings.heading_color != 'rgba(0,0,0,0)'
                        assign inline_css = inline_css | append: 'color:' | append: block.settings.heading_color | append: ';'
                      endif
                    -%}
                    <h3
                      class="{{ block.settings.heading_size }} text-current"
                      {% if inline_css != blank %}
                        style="{{ inline_css }}"
                      {% endif %}
                    >
                      <motion-element
                        data-motion="{{ animation_effect }}"
                        data-motion-delay="{{ motion_delay }}"
                        class="block"
                      >
                        {%- render 'highlight-text',
                          hl_text: block.settings.title,
                          hl_style: block.settings.highlight_style,
                          enable_hl_text_color: block.settings.enable_highlight_text_color
                        -%}
                      </motion-element>
                    </h3>
                    {%- assign motion_delay = motion_delay | plus: 50 -%}
                  {%- endif -%}
                  {%- if block.settings.text != blank -%}
                    <motion-element
                      data-motion="{{ animation_effect }}"
                      data-motion-delay="{{ motion_delay }}"
                      class="block mega-description rte"
                    >
                      {{ block.settings.text }}
                    </motion-element>
                    {%- assign motion_delay = motion_delay | plus: 50 -%}
                  {%- endif -%}
                  {%- if block.settings.button_label != blank -%}
                    <motion-element
                      data-motion="{{ animation_effect }}"
                      data-motion-delay="{{ motion_delay }}"
                      class="f-text-blocks__btn"
                    >
                      <a
                        class="btn {{ block.settings.button_style }}"
                        {% if block.settings.button_link != blank %}
                          href="{{ block.settings.button_link }}"
                        {% else %}
                          aria-disabled="true"
                          disabled
                        {% endif %}
                      >
                        {{- block.settings.button_label -}}
                      </a>
                    </motion-element>
                    {%- assign motion_delay = motion_delay | plus: 50 -%}
                  {%- endif -%}
                </div>
              {%- when 'text_box' -%}
                {%- liquid
                  assign items = block.settings.scrolling_promotion | newline_to_br | split: '<br />'
                  assign duration = 6.0
                  assign duration = duration | divided_by: block.settings.speed
                  if block.settings.direction == 'left'
                    assign direction_reverse = 'right'
                  else
                    assign direction_reverse = 'left'
                  endif
                -%}
                {%- if items != blank -%}
                  {{-
                    'scrolling-promotion.css'
                    | asset_url
                    | stylesheet_tag: preload: true
                  -}}
                {%- endif %}
                <div class="f-custom__block-wrapper relative w-full h-full flex flex-col{% if items != blank %} justify-between{% else %} justify-center{% endif %} color-{{ block.settings.color_schema }}">
                  {%- if items != blank -%}
                    <f-scrolling-promotion
                      class="f-scrolling-promotion f-scrolling-promotion--top f-scrolling-promotion--{{ block.settings.direction }} text-sm"
                      style="--item-gap: 15px; --item-gap-mobile:15px; --duration: {{ duration }}s;"
                    >
                      <div class="f-promotion items-center">
                        {%- for item in items -%}
                          <div class="f-promotion__item">{{ item }}</div>
                        {%- endfor -%}
                      </div>
                    </f-scrolling-promotion>
                  {%- endif -%}
                  <div class="f-text-blocks w-full">
                    {%- if block.settings.subheading != blank -%}
                      <motion-element
                        data-motion="{{ animation_effect }}"
                        data-motion-delay="{{ motion_delay }}"
                        class="block mega-subtitle"
                      >
                        {{ block.settings.subheading }}
                      </motion-element>
                      {%- assign motion_delay = motion_delay | plus: 50 -%}
                    {%- endif -%}
                    {% if block.settings.title != blank %}
                      {%- liquid
                        assign inline_css = ''

                        if block.settings.heading_color != blank and block.settings.heading_color != 'rgba(0,0,0,0)'
                          assign inline_css = inline_css | append: 'color:' | append: block.settings.heading_color | append: ';'
                        endif
                      -%}
                      <h3
                        class="{{ block.settings.heading_size }}"
                        {% if inline_css != blank %}
                          style="{{ inline_css }}"
                        {% endif %}
                      >
                        <motion-element
                          data-motion="{{ animation_effect }}"
                          data-motion-delay="{{ motion_delay }}"
                          class="block"
                        >
                          {%- render 'highlight-text',
                            hl_text: block.settings.title,
                            hl_style: block.settings.highlight_style,
                            enable_hl_text_color: block.settings.enable_highlight_text_color
                          -%}
                        </motion-element>
                      </h3>
                      {%- assign motion_delay = motion_delay | plus: 50 -%}
                    {% endif %}
                    {%- if block.settings.text != blank -%}
                      <motion-element
                        data-motion="{{ animation_effect }}"
                        data-motion-delay="{{ motion_delay }}"
                        class="block mega-description rte"
                      >
                        {{ block.settings.text }}
                      </motion-element>
                      {%- assign motion_delay = motion_delay | plus: 50 -%}
                    {%- endif -%}
                    {%- if block.settings.button_label != blank -%}
                      <motion-element
                        data-motion="{{ animation_effect }}"
                        data-motion-delay="{{ motion_delay }}"
                        class="f-text-blocks__btn"
                      >
                        <a
                          class="btn {{ block.settings.button_style }}"
                          {% if block.settings.button_link != blank %}
                            href="{{ block.settings.button_link }}"
                          {% else %}
                            aria-disabled="true"
                            disabled
                          {% endif %}
                        >
                          {{- block.settings.button_label -}}
                        </a>
                      </motion-element>
                      {%- assign motion_delay = motion_delay | plus: 50 -%}
                    {%- endif -%}
                  </div>
                  {%- if items != blank -%}
                    <f-scrolling-promotion
                      class="f-scrolling-promotion f-scrolling-promotion--top f-scrolling-promotion--{{ direction_reverse }} text-sm"
                      style="--item-gap: 15px; --item-gap-mobile:15px; --duration: {{ duration }}s;"
                    >
                      <div class="f-promotion items-center">
                        {%- for item in items -%}
                          <div class="f-promotion__item">{{ item }}</div>
                        {%- endfor -%}
                      </div>
                    </f-scrolling-promotion>
                  {%- endif -%}
                </div>
              {%- when 'video' -%}
                {%- liquid
                  assign video_link = block.settings.video_link
                  assign cover_image = block.settings.image

                  if cover_image == blank and block.settings.video_type == 'shopify'
                    assign cover_image = block.settings.shopify_video.preview_image
                  endif

                  if block.settings.video_ratio == 'adapt'
                    assign video_aspect_ratio = cover_image.aspect_ratio | default: '16/9'
                  else
                    assign video_aspect_ratio = block.settings.video_ratio | replace: ':', '/'
                  endif
                -%}
                <div class="f-video w-full">
                  <motion-element
                    data-motion="{{ animation_effect }}"
                    class="block f-video__wrapper overflow-hidden relative"
                  >
                    <noscript>
                      <div class="f-video__media">
                        <a
                          href="{{ video_link }}"
                          class="video-section__poster"
                          style="--aspect-ratio: {{ video_aspect_ratio }}"
                        >
                          {%- if cover_image != blank -%}
                            <img
                              srcset="
                                {%- if cover_image.width >= 375 -%}{{ cover_image | image_url: width: 375 }} 375w,{%- endif -%}
                                {%- if cover_image.width >= 750 -%}{{ cover_image | image_url: width: 750 }} 750w,{%- endif -%}
                                {%- if cover_image.width >= 1100 -%}{{ cover_image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                {%- if cover_image.width >= 1500 -%}{{ cover_image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                {%- if cover_image.width >= 1780 -%}{{ cover_image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                {%- if cover_image.width >= 2000 -%}{{ cover_image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                {%- if cover_image.width >= 3000 -%}{{ cover_image | image_url: width: 3000 }} 3000w,{%- endif -%}
                                {%- if cover_image.width >= 3840 -%}{{ cover_image | image_url: width: 3840 }} 3840w,{%- endif -%}
                                {{ cover_image | image_url }} {{ cover_image.width }}w
                              "
                              src="{{ cover_image | image_url: width: 1920 }}"
                              sizes="(min-width: {{ settings.container_width }}px) {{ settings.container_width | minus: 100 }}px, (min-width: 750px) calc(100vw - 10rem), 100vw"
                              alt="{{ 'sections.video.load_video' | t: description: section.settings.description | escape }}"
                              loading="lazy"
                              width="{{ cover_image.width }}"
                              height="{{ cover_image.height }}"
                            >
                          {%- else -%}
                            {{
                              'lifestyle-2'
                              | placeholder_svg_tag: 'placeholder-svg placeholder h-full w-full'
                            }}
                          {%- endif -%}
                          <span class="deferred-media__poster-button motion-reduce">
                            {% comment %} theme-check-disable {% endcomment %}
                            {{ 'icon-play.svg' | inline_asset_content }}
                            {% comment %} theme-check-enable {% endcomment %}
                          </span>
                        </a>
                      </div>
                    </noscript>
                    <deferred-media
                      class="f-video__media deferred-media no-js-hidden"
                      data-autoplay="{{ block.settings.autoplay_video }}"
                      data-media-id="{{ video_link.id }}"
                      style="--aspect-ratio: {{ video_aspect_ratio }}"
                    >
                      <button
                        id="Deferred-Poster-Modal-{{ video_link.id }}"
                        class="f-video__poster media deferred-media__poster{% if animation_effect == 'zoom-fade' %} f-image{% endif %}"
                        type="button"
                        aria-label="{{ 'accessibility.load_video' | t }}"
                      >
                        {%- if cover_image != blank -%}
                          <div class="f-video__cover bg-image">
                            {% render 'responsive-image',
                              image: cover_image,
                              section_index: section.index,
                              index: forloop.index
                            %}
                          </div>
                        {%- else -%}
                          {{
                            'lifestyle-2'
                            | placeholder_svg_tag: 'placeholder-svg placeholder h-full w-full'
                          }}
                        {%- endif -%}
                        <span class="deferred-media__poster-button motion-reduce">
                          {% comment %} theme-check-disable {% endcomment %}
                          {{ 'icon-play.svg' | inline_asset_content }}
                          {% comment %} theme-check-enable {% endcomment %}
                        </span>
                      </button>
                      <template>
                        {%- if block.settings.video_type == 'external' -%}
                          {%- if video_link.type == 'youtube' -%}
                            <iframe
                              src="https://www.youtube.com/embed/{{ video_link.id }}?enablejsapi=1&autoplay=1{% if block.settings.autoplay_video %}&mute=1&loop=1&playlist={{ video_link.id }}{% endif %}"
                              class="js-youtube"
                              allow="autoplay; encrypted-media"
                              allowfullscreen
                            ></iframe>
                          {%- else -%}
                            <iframe
                              src="https://player.vimeo.com/video/{{ video_link.id }}?autoplay=1{% if block.settings.autoplay_video %}&muted=1&loop=1{% endif %}"
                              class="js-vimeo"
                              allow="autoplay; encrypted-media"
                              allowfullscreen
                            ></iframe>
                          {%- endif -%}
                        {%- else -%}
                          {%- liquid
                            assign loop = false
                            assign muted = false
                            assign autoplay = false
                            if block.settings.autoplay_video
                              assign loop = true
                              assign muted = true
                              assign autoplay = true
                            endif
                          -%}
                          {% if block.settings.shopify_video %}
                            {{-
                              block.settings.shopify_video
                              | video_tag:
                                playsinline: true,
                                muted: muted,
                                autoplay: autoplay,
                                loop: loop,
                                controls: true,
                                image_size: '800x'
                            -}}
                          {%- else -%}
                            <video
                              src="https://cdn.shopify.com/videos/c/o/v/fbb140fd46e3429eba459e1549dbbe38.mp4"
                              playsinline
                              controls
                              {% if block.settings.autoplay_video %}
                                autoplay loop muted
                              {% endif %}
                            ></video>
                          {%- endif -%}
                        {%- endif -%}
                      </template>
                    </deferred-media>
                  </motion-element>
                </div>
              {%- when 'product' -%}
                {%- liquid
                  assign product = all_products[block.settings.product]
                  assign image_ratio = block.settings.pcard_image_ratio
                  assign show_vendor = block.settings.show_vendor
                -%}
                <motion-element
                  data-motion="{{ animation_effect }}"
                  class="block w-full"
                >
                  {%- if block.settings.subheading != blank
                    or block.settings.heading != blank
                    or block.settings.text != blank
                  -%}
                    <div class="f-text-blocks text-center py-sm sm:py-md sm:pt-0">
                      {%- if block.settings.subheading != blank -%}
                        <div class="mega-subtitle">
                          {{ block.settings.subheading }}
                        </div>
                      {%- endif -%}
                      {%- if block.settings.heading != blank -%}
                        <h3>
                          {%- render 'highlight-text',
                            hl_text: block.settings.heading,
                            hl_style: block.settings.highlight_style,
                            enable_hl_text_color: block.settings.enable_highlight_text_color
                          -%}
                        </h3>
                      {%- endif -%}
                      {%- if block.settings.text != blank -%}
                        <div class="mega-description rte">
                          {{ block.settings.text }}
                        </div>
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                  {% if product != blank %}
                    {% render 'product-card',
                      color_schema: section.settings.color_schema,
                      product: product,
                      image_ratio: image_ratio,
                      show_vendor: show_vendor,
                      section_index: section.index,
                      index: forloop.index
                    %}
                  {%- else -%}
                    {% render 'product-card-placeholder',
                      index: forloop.index
                    %}
                  {%- endif -%}
                </motion-element>
              {%- when 'article' -%}
                <motion-element
                  data-motion="{{ animation_effect }}"
                  class="block"
                >
                  {{ 'blog.css' | asset_url | stylesheet_tag }}
                  {%- liquid
                    assign article = articles[block.settings.article]
                    assign show_date = block.settings.show_date
                    assign show_excerpt = block.settings.show_excerpt
                    assign show_readmore = block.settings.show_readmore
                    assign image_aspect_ratio = block.settings.image_aspect_ratio
                  -%}
                  {%- if article != empty -%}
                    {%- render 'article-card',
                      article: article,
                      image_aspect_ratio: image_aspect_ratio,
                      show_date: show_date,
                      show_excerpt: show_excerpt,
                      show_readmore: show_readmore,
                      classes: 'w-full',
                      index: forloop.index
                    -%}
                  {%- else -%}
                    {%- liquid
                      if image_aspect_ratio == 'adapt'
                        assign image_aspect_ratio = '1/1'
                      endif
                    -%}
                    {% render 'article-card-placeholder',
                      image_aspect_ratio: image_aspect_ratio,
                      classes: 'w-full',
                      show_excerpt: true,
                      show_readmore: true,
                      index: forloop.index
                    %}
                  {%- endif -%}
                </motion-element>
              {%- when 'lookbook' -%}
                <motion-element
                  data-motion="{{ animation_effect }}"
                  data-motion-delay="{{ motion_delay }}"
                  class="block w-full"
                >
                  {{
                    'component-lookbook-card.css'
                    | asset_url
                    | stylesheet_tag
                  }}
                  {% render 'lookbook-card',
                    color_schema: section.settings.color_schema,
                    block: block,
                    image_aspect_ratio: 'adapt',
                    custom_class: 'w-full',
                    index: forloop.index
                  %}
                  <script
                    src="{{ 'lookbook-icon.js' | asset_url }}"
                    defer="defer"
                  ></script>
                </motion-element>
              {%- when 'product_bundles' -%}
                <script
                  src="{{ 'product-bundle.js' | asset_url }}"
                  defer="defer"
                ></script>
                <custom-product-bundle class="w-full no-js-hidden overflow-hidden block">
                  <div class="product-bundle">
                    {%- if block.settings.subheading != blank -%}
                      <motion-element
                        data-motion="{{ animation_effect }}"
                        data-motion-delay="{{ motion_delay }}"
                        class="block mega-subtitle"
                      >
                        {{- block.settings.subheading -}}
                      </motion-element>
                      {%- assign motion_delay = motion_delay | plus: 50 -%}
                    {%- endif -%}
                    {%- if block.settings.title != blank -%}
                      <h3 class="{{ block.settings.heading_size }} font-bold">
                        <motion-element
                          data-motion="{{ animation_effect }}"
                          data-motion-delay="{{ motion_delay }}"
                          class="block"
                        >
                          {%- render 'highlight-text',
                            hl_text: block.settings.title,
                            hl_style: block.settings.highlight_style,
                            enable_hl_text_color: block.settings.enable_highlight_text_color
                          -%}
                        </motion-element>
                      </h3>
                      {%- assign motion_delay = motion_delay | plus: 50 -%}
                    {%- endif -%}
                    <div class="product-bundle__list">
                      {%- liquid
                        assign products = 'product_1,product_2,product_3,product_4,product_5' | split: ','
                        assign total_price = 0
                        assign saved_price = 0
                      %}
                      {%- for product_id in products -%}
                        {%- liquid
                          assign product = all_products[block.settings[product_id]]
                          assign variant = product.selected_or_first_available_variant
                          assign compare_at_price = variant.compare_at_price
                          assign price = variant.price | default: 1999
                          assign money_price = price | money
                        %}
                        {%- if product != empty -%}
                          {%- liquid
                            assign total_price = total_price | plus: variant.price
